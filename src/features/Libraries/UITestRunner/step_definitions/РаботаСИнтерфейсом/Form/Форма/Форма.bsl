&НаКлиенте
Функция НайтиЭлементНаФорме(ИмяЭлемента, ТипЭлемента = Неопределено, ИскатьПоИмени, Таймаут = 0)

	МассивФорм = НайтиФормыТекущегоОкна();
	
	Для каждого ТекущаяФорма Из МассивФорм Цикл
		
		Если ИскатьПоИмени Тогда
			ЭлементФормы = ТекущаяФорма.НайтиОбъект(ТипЭлемента,, ИмяЭлемента, Таймаут);
		Иначе
			ЭлементФормы = ТекущаяФорма.НайтиОбъект(ТипЭлемента, ИмяЭлемента,, Таймаут);
		КонецЕсли;
		
		Если ЭлементФормы = Неопределено Тогда
			Продолжить;
		КонецЕсли;
		
		Прервать;
		
	КонецЦикла;
	
	Возврат ЭлементФормы;

КонецФункции // НайтиЭлементНаФорме()

&НаКлиенте
Процедура ВОткрытойФормеЯЖдуПоявленияЭлементаВТечениеСекунд(ИмяЭлемента, Таймаут = 10, ИскатьПоИмени = Ложь)
	
	Нашли = Ложь;
	ЭлементФормы = НайтиЭлементНаФорме(ИмяЭлемента,, ИскатьПоИмени, Таймаут);
	Если ЭлементФормы <> Неопределено Тогда
		Нашли = Истина;
	КонецЕсли;
	
	Если НЕ Нашли Тогда
		Если ИскатьПоИмени Тогда
			Стр = "Элемент с именем <" + ИмяЭлемента + "> не появился в текущей форме в течение <" + Таймаут + "> секунд!";
		Иначе	
			Стр = "Элемент с заголовком <" + ИмяЭлемента + "> не появился в текущей форме в течение <" + Таймаут + "> секунд!";
		КонецЕсли;	 
		Сообщить(Стр);
		ВызватьИсключение Стр;
	КонецЕсли;
	
КонецПроцедуры

